{
  "name": "leetcode",
  "version": "1.0.1",
  "author": "hukun",
  "type": "module",
  "scripts": {
    "release": "standard-version",
    "up1": "node generate.js update-change",
    "up2": "node generate.js update-readme",
    "commit": "npm run up2 && npm run up1"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(changelog): {{currentTag}}"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
